<Window x:Class="_13_WPF_13_Password_Box.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_13_WPF_13_Password_Box"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel Margin="10 15 10 15">
        <Label>Username:</Label>
        <TextBox Name="tbUsername"></TextBox>
        <Label>Password:</Label>
        <PasswordBox PasswordChar="O" MaxLength="10" Name="pbPassword"></PasswordBox>
        <Button Margin="5" Content="Login" Click="Button_Click"/>
    </StackPanel>
</Window>
